<!DOCTYPE html>
<html>
  <head>
    <title>LOGIN</title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <form class="inner" action="/vote_info_check" method="POST">
      <h1>Redirecting...</h1>
      <%= token %>
      <input type="hidden" name="token" value="<%= token %>">
      <input type="hidden" name="r_id" value="<%= r_id %>">
      <input type="submit" value="submit" id="autosubmit">
    </form>
  </body>
  <script>document.querySelector('#autosubmit').click()</script>
</html>
